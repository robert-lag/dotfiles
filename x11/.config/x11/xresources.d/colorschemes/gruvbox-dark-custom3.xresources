#define black_dark #282828
#define black_light #363636
#define red_dark #cc241d
#define red_light #fb4934
#define green_dark #98971a
#define green_light #b8bb26
#define yellow_dark #d79921
#define yellow_light #fabd2f
#define blue_dark #458588
#define blue_light #83a598
