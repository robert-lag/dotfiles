*color1:  red_dark
*color9:  red_light
! DarkGreen + Green
*color2:  green_dark
*color10: green_light
! DarkYellow + Yellow
*color3:  yellow_dark
*color11: yellow_light
! DarkBlue + Blue
*color4:  blue_dark
*color12: blue_light
! DarkMagenta + Magenta
*color5:  magenta_dark
*color13: magenta_light
! DarkCyan + Cyan
*color6:  cyan_dark
*color14: cyan_light
! LightGrey + White
*color7:  white_dark
*color15: white_light
